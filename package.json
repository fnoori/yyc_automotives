{
  "name": "yycauto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"npm run dev-client-local\" \"npm run dev-server-local\"",
    "dev-aws": "concurrently \"npm run dev-server-aws\" \"npm run dev-client-aws\"",
    "dev-client": "NODE_ENV=development vue-cli-service serve",
    "dev-client-aws": "NODE_ENV=development-aws vue-cli-service serve",
    "dev-client-local": "NODE_ENV=development-local vue-cli-service serve",
    "dev-server": "NODE_ENV=development nodemon src/server/main.js",
    "dev-server-aws": "NODE_ENV=development-aws nodemon src/server/main.js",
    "dev-server-local": "NODE_ENV=development-local nodemon src/server/main.js",
    "debug-local": "NODE_ENV=development-local node --inspect --inspect-brk=3000 src/server/main.js",
    "debug-aws": "NODE_ENV=development-aws node --inspect --inspect-brk=3000 src/server/main.js"
  },
  "dependencies": {
    "argon2": "^0.21.0",
    "aws-sdk": "^2.429.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.16",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dot-object": "^1.7.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.4.2",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "quasar": "^1.0.0-beta.13",
    "sass-loader": "^7.1.0",
    "serve-static": "^1.13.2",
    "underscore": "^1.9.1",
    "validator": "^10.11.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "camelcase": "off",
      "no-useless-escape": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
